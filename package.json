{
  "name": "@react-hookz/web",
  "version": "23.0.1",
  "description": "React hooks done right, for browser and SSR.",
  "keywords": [
    "react",
    "hook",
    "react-hook",
    "browser",
    "ssr"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/react-hookz/web.git"
  },
  "bugs": {
    "url": "https://github.com/react-hookz/web/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "sideEffects": false,
  "files": [
    "cjs",
    "esm"
  ],
  "imports": {
    "#root/*": [
      "./src/*"
    ]
  },
  "scripts": {
    "prepare": "yarn husky install",
    "commit": "yarn git-cz",
    "build": "yarn build:cleanup && concurrently yarn:build:cjs yarn:build:esm",
    "build:cleanup": "yarn rimraf ./dist",
    "build:cjs": "yarn tsc -p ./tsconfig.build.json --module CommonJS --outDir ./cjs",
    "build:esm": "yarn tsc -p ./tsconfig.build.json --module ESNext --outDir ./esm",
    "new-hook": "node utility/add-new-hook.cjs",
    "test": "yarn jest --selectProjects dom ssr",
    "test:coverage": "yarn test --coverage",
    "lint": "yarn eslint --ext .js,.ts,.jsx,.tsx,.md,.mdx ./",
    "lint:fix": "yarn lint --fix"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md}": "yarn eslint --ext .js,.ts,.jsx,.tsx,.md,.mdx --fix"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  },
  "dependencies": {
    "@react-hookz/deep-equal": "^1.0.4"
  },
  "peerDependencies": {
    "js-cookie": "^3.0.5",
    "react": "^16.8 || ^17 || ^18",
    "react-dom": "^16.8 || ^17 || ^18"
  },
  "peerDependenciesMeta": {
    "js-cookie": {
      "optional": true
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@commitlint/cz-commitlint": "^17.5.0",
    "@jamesacarr/jest-reporter-github-actions": "^0.0.4",
    "@react-hookz/eslint-config": "^1.8.0",
    "@react-hookz/eslint-formatter-gha": "^1.0.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.0.3",
    "@testing-library/dom": "^9.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^29.5.2",
    "@types/js-cookie": "^3.0.3",
    "@types/react": "^17.0.61",
    "@types/react-dom": "^17.0.20",
    "commitizen": "^4.3.0",
    "commitlint": "^17.6.5",
    "concurrently": "^8.2.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "js-cookie": "^3.0.5",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^5.0.1",
    "semantic-release": "^21.0.5",
    "ts-jest": "^29.1.0",
    "ts-jest-resolver": "^2.0.1",
    "typescript": "^5.1.3",
    "yarn": "^1.22.19"
  }
}
